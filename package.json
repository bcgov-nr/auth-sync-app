{
  "name": "authtool",
  "version": "1.0.0",
  "author": "Matthew Bystedt <matthew.bystedt@gov.bc.ca>",
  "description": "Auth sync tool for IIT",
  "bin": {
    "authtool": "./bin/run"
  },
  "dependencies": {
    "@oclif/core": "^2.15.0",
    "@oclif/plugin-help": "^5.2.19",
    "@oclif/plugin-plugins": "^3.6.1",
    "axios": "^1.5.0",
    "inversify": "^6.0.1",
    "jira-client": "^8.2.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^2.5.4",
    "@types/chai": "^4.3.6",
    "@types/ejs": "^3.1.2",
    "@types/jira-client": "^7.1.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.17.15",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "chai": "^4.3.8",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "oclif": "^3.15.1",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "authtool",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc.json",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "pack": "",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "manifest": "oclif manifest && oclif readme",
    "test": "jest",
    "e2e": "jest --config=jest.e2e.config.js",
    "version": "oclif readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://apps.nrs.gov.bc.ca/int/stash/scm/appdev/vault-policy.git"
  },
  "types": "dist/index.d.ts"
}
