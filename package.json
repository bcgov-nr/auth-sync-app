{
  "name": "authtool",
  "version": "1.0.0",
  "author": "Matthew Bystedt <matthew.bystedt@gov.bc.ca>",
  "description": "Auth sync tool for IIT",
  "bin": {
    "authtool": "./bin/run"
  },
  "dependencies": {
    "@oclif/core": "^3.15.0",
    "@oclif/plugin-help": "^6.0.9",
    "@oclif/plugin-plugins": "^4.1.10",
    "axios": "^1.6.4",
    "inversify": "^6.0.2",
    "jira-client": "^8.2.2",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^3.1.5",
    "@types/chai": "^4.3.11",
    "@types/ejs": "^3.1.5",
    "@types/jira-client": "^7.1.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "chai": "^4.3.10",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "oclif": "^4.1.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "authtool",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc.json",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "pack": "",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "manifest": "oclif manifest && oclif readme",
    "test": "jest",
    "e2e": "jest --config=jest.e2e.config.js",
    "version": "oclif readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://apps.nrs.gov.bc.ca/int/stash/scm/appdev/vault-policy.git"
  },
  "types": "dist/index.d.ts"
}
