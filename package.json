{
  "name": "authtool",
  "version": "1.0.0",
  "author": "Matthew Bystedt <matthew.bystedt@gov.bc.ca>",
  "description": "Auth sync tool for IIT",
  "bin": {
    "authtool": "./bin/run"
  },
  "dependencies": {
    "@oclif/core": "^4.0.17",
    "@oclif/plugin-help": "^6.2.8",
    "@oclif/plugin-plugins": "^5.4.4",
    "axios": "^1.7.4",
    "inversify": "^6.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@oclif/test": "^4.0.8",
    "@types/chai": "^4.3.17",
    "@types/ejs": "^3.1.5",
    "@types/eslint__js": "^8.42.3",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.4.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "chai": "^5.1.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "mocha": "^10.7.3",
    "oclif": "^4.14.19",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "authtool",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint \"src/**/*.ts\"",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "pack": "",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "manifest": "oclif manifest && oclif readme",
    "test": "jest",
    "e2e": "jest --config=jest.e2e.config.js",
    "version": "oclif readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://apps.nrs.gov.bc.ca/int/stash/scm/appdev/vault-policy.git"
  },
  "types": "dist/index.d.ts"
}
