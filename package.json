{
  "name": "auth-sync-app",
  "version": "1.0.0",
  "author": "Matthew Bystedt <matthew.bystedt@gov.bc.ca>",
  "description": "Auth Sync App keeps target system roles up-to-date",
  "bin": {
    "auth-sync-app": "./bin/run.js"
  },
  "type": "module",
  "dependencies": {
    "@oclif/core": "^4.0.33",
    "@oclif/plugin-help": "^6.2.18",
    "@oclif/plugin-plugins": "^5.4.17",
    "@octokit/auth-app": "^7.1.3",
    "@octokit/core": "^6.1.2",
    "@octokit/oauth-app": "^7.1.3",
    "axios": "^1.7.7",
    "inversify": "^6.1.4",
    "nodemailer": "^6.9.16",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.2",
    "@types/chai": "file:@types/chai",
    "@types/ejs": "^3.1.5",
    "@types/eslint__js": "^8.42.3",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.9.1",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "chai": "^5.1.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "mocha": "^10.8.2",
    "oclif": "^4.15.28",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "!**/*.tsbuildinfo",
    "LICENSE",
    "README.md"
  ],
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "authtool",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint \"src/**/*.ts\"",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "pack": "",
    "prepack": "oclif manifest && oclif readme",
    "test": "jest",
    "e2e": "jest --config=jest.e2e.config.js",
    "version": "oclif readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://apps.nrs.gov.bc.ca/int/stash/scm/appdev/vault-policy.git"
  },
  "types": "dist/index.d.ts"
}
